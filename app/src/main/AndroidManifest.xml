<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.android.golocalfinal">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="GoLocal"
        android:roundIcon="@drawable/appiconround"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".EditBuyerProfile"></activity>
        <activity android:name=".ViewMySpecificOrder"></activity>


        <activity android:name=".ForgotPassword"></activity>

        <activity android:name=".ViewBuyerProfile" />
        <activity android:name=".BuyerOrders" />
        <activity android:name=".Favourites" />
        <activity android:name=".ViewCart" />
        <activity
            android:name=".OrdersReceived"
            android:label="@string/title_activity_orders_received1"
            android:parentActivityName=".AfterLoginSeller"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditSellerProfile"
            android:label=""
            android:parentActivityName=".ViewSellerProfile" />
        <activity
            android:name=".ViewSellerProfile"
            android:label=""
            android:parentActivityName=".AfterLoginSeller" />
        <activity
            android:name=".Developers"
            android:label="Developers Info" />
        <activity
            android:name=".About"
            android:label="About" />
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".SplashScreen" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:label="Sign Up" />
        <activity
            android:name=".AfterLoginSeller"
            android:label="" />
        <activity
            android:name=".AfterLoginBuyer"
            android:label="" />
        <activity android:name=".SellerBasicInfo" />
        <activity android:name=".BuyerBasicInfo" />
        <activity
            android:name=".CategorySpecificInfo"
            android:label=""
            android:parentActivityName=".AfterLoginSeller" />
        <activity
            android:name=".ShopSpecificInfo"
            tools:ignore="DuplicateActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>