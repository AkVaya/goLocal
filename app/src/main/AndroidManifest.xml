<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.golocalfinal">

    <application
        android:allowBackup="true"
        android:icon="@drawable/vector"
        android:label="GoLocal"
        android:roundIcon="@drawable/vector"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:parentActivityName=".SplashScreen"/>
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"
            android:label="Sign Up"></activity>
        <activity android:name=".AfterLoginSeller"
            ></activity>
        <activity android:name=".AfterLoginBuyer"></activity>
        <activity android:name=".SellerBasicInfo"></activity>
        <activity android:name=".BuyerBasicInfo"></activity>
        <activity android:name=".CategorySpecificInfo"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>